# Uncomment the next line to define a global platform for your project
install! 'cocoapods',
  :preserve_pod_file_structure => true

platform :ios, '16.0'

target 'FlickrApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FlickerApp
   #pod 'Kingfisher'
  pod 'Kingfisher', :git => 'https://github.com/Aelx-Vaiman/Kingfisher.git'
  pod 'ChocKit', :git => 'https://github.com/Aelx-Vaiman/ChocKit.git', :branch => 'development'
#pod 'ChocKit', '0.1.5'
end
